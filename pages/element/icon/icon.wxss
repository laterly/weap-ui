/* pages/element/icon/icon.wxss */
.icon-box{
    display: flex;
    flex-flow: wrap;
    margin-top: 30rpx;
    box-sizing: border-box;
}
.icon-list{
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 30rpx 0;
    width: 33.33%;
}
.icon-text{
    font-size: 14px;
    color: #333;
    margin-top: 15rpx;
}