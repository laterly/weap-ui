/* list.wxss */
.list{
    height:100rpx;
    background:rgba(255,255,255,1);
    display: flex;
    align-items: center;
    width: 100%;
    padding: 0 35rpx;
    box-sizing: border-box;
}
.hover-active{
    background-color: #f5f5f5;
}
.left-img{
    width: 30rpx;
    height: 30rpx;
    object-fit: cover;
}
.left{
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 10rpx;
}
.center{
   display: flex;
   align-items: center;
   justify-content: space-between;
   flex: 1;
}
.center-title{
    font-size:32rpx;
    font-weight:400;
    color:rgba(0,0,0,1); 
}
.center-right{
    font-size:24rpx;
    font-weight:400;
    color:rgba(204,204,204,1);
    display: flex;
    align-items: center;
}
.right-icon{
    width: 15rpx;
    height: 28rpx;
    margin-left: 10rpx;
    object-fit: cover;
}